export const APP_MODE = (typeof window !== 'undefined' && window.__app_mode__) || process.env.APP_MODE;

export function lazy(dynamicImport, isRouteComponent?: Boolean): any {
  if (isRouteComponent) {
    return {
      __LAZY__: true,
      dynamicImport,
    };
  } else {
    return React.lazy(dynamicImport);
  }
}

